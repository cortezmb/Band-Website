<!DOCTYPE html>
<html>

<head>
   <%- include('partials/template/head.ejs')  %> 

</head>

<body id="speakerList">
    <%- include('partials/content/header.ejs')  %> 
    <div class="container">
        <div class="row">
            <div class="col-sm-8">
                <article class="speakerslist ">
                    <div class="container">
                        <div class="row">

                            <% albums.forEach(albumsObj => { %>
                             
                            
                            <div class="col-sm-8 ">
                                <h3 class="speakerslist-title"><%= albumsObj.title %> </h3>
                                <h5 class="speakerslist-name"><a href="/albums/<%= albumsObj.shortname %> "><%= albumsObj.albumname %> </a>
                                </h5>
                                <p class="speakerslist-info">
                                    <a href="/albums/<%= albumsObj.shortname %> ">

                                        <img class="speakerslist-img img-circle pull-left"
                                            src="/images/header/<%= albumsObj.shortname %>_tn.jpg"     
                                            alt="Photo of <%= albumsObj.name %> "></a>

                                    </a>


                                    </a>

                                    <%= albumsObj.summary %> 

                                </p>
                            </div>

                            <% }) %>

                        </div><!-- row -->
                    </div><!-- container -->
                </article>
            </div><!-- primary -->
            <aside class="col-sm-4">
                <article class="sidebar">
                    <h1 class="sidebar-title">Albums</h1>
                    <p class="sidebar-body">words go here</p>
                    
                        <%- include('partials/content/artwork.ejs')  %> 
                </article>
            </aside><!-- secondary -->
        </div>
    </div><!-- container -->
    
    <%- include('partials/template/jsdefaults.ejs')  %> 

</body>

</html>